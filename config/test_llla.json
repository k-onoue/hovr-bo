{
    "device": "cpu",
    "dtype": "float64",
    "n_initial_eval": 3,
    "initial_sample_method": "sobol", 
    "n_iter": 10,
    "batch_size": 1,
    "is_maximize": false,
    "objective": {
        "function": "synthetic",
        "noise_std": 1.0,
        "outlier_prob": 0.1,
        "outlier_scale": 10.0,
        "outlier_std": 1.0
    },
    "sampler": {
        "function": "llla_artl",
        "acqf": "log_ei",
        "sampler_args": {
            "regnet_activation": "tanh",
            "loss_params": {
                "n_epochs": 10000,
                "lr": 1e-2,
                "weight_decay": 0,
                "artl_weight": 1e-1,
                "lambd": 0,
                "k": [3],
                "q": 2,
                "M": 2
            }
        }
    }
}
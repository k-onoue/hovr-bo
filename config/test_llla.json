{
    "device": "cpu",
    "dtype": "float64",
    "n_initial_eval": 1,
    "n_iter": 40,
    "batch_size": 1,
    "is_maximize": false,
    "objective": {
        "function": "ackley2",
        "noise_std": 1.0,
        "outlier_prob": 0.0,
        "outlier_scale": 10.0,
        "outlier_std": 1.0
    },
    "sampler": {
        "function": "llla_artl",
        "acqf": "log_ei",
        "sampler_args": {
            "loss_params": {
                "n_epochs": 1000,
                "lr": 1e-2,
                "weight_decay": 0,
                "artl_weight": 1e-3,
                "lambda": 1e-3,
                "k": [3],
                "q": 1,
                "M": 10
            }
        }
    }
}